<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_enabled="false"
        android:drawable="@drawable/btn_bg_enabled_false"/>
    <item
        android:state_pressed="true"
        android:state_enabled="true"
        android:drawable="@drawable/btn_bg_blue_square_pressed"/>
    <item
        android:state_focused="true"
        android:state_enabled="true"
        android:drawable="@drawable/btn_bg_focused_true"/>
    <item
        android:state_enabled="true"
        android:drawable="@drawable/btn_bg_enabled_true"/>
</selector>